    "passport-twitter": "^1.0.3",
    "superagent": "^1.2.0",
    "underscore.string": "^3.2.2"
  },
  "devDependencies": {
    "body-parser": "*",
    "cookie-parser": "*",
    "cookie-session": "*",
    "morgan": "*",
    "mocha": "*",
    "should": "*",
    "sinon": "*",
    "jade": "*",
    "underscore": "*",
    "backbone": "*",
    "sharify": "*",
    "backbone-super-sync": "*",
    "browserify": "*",
    "coffeeify": "*",
    "artsy-xapp": "*",
    "zombie": "*",
    "rewire": "2.2.0"
  }
