    "coffeeify": "2.1.0",
    "cookie-parser": "^1.4.5",
    "cookie-session": "^1.4.0",
    "eslint": "^7.30.0",
    "eslint-plugin-mocha": "^8.0.0",
    "jade": "^1.11.0",
    "mocha": "^7.2.0",
    "morgan": "^1.10.0",
    "rewire": "^2.2.0",
    "sharify": "^0.1.6",
    "should": "^13.2.3",
    "sinon": "^11.1.2"
  }
